{
  "name": "sdbc",
  "version": "1.2.0",
  "description": "Mongoose tarzı tek API ile birden fazla veritabanı kullanın - MongoDB, PostgreSQL, MySQL, SQLite",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "sdbc": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/cli.ts --format cjs,esm --dts --clean",
    "build:cli": "tsup src/cli.ts --format cjs --clean --outDir dist",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "mongoose",
    "mongodb",
    "postgresql",
    "mysql",
    "sqlite",
    "orm",
    "database",
    "adapter",
    "universal",
    "multi-database",
    "sdbc"
  ],
  "author": "",
  "license": "MIT",
  "homepage": "https://github.com/seyfooksck/sdbc#readme",
  "bugs": {
    "url": "https://github.com/seyfooksck/sdbc/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seyfooksck/sdbc.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "peerDependencies": {
    "better-sqlite3": ">=9.0.0",
    "mongodb": ">=5.0.0",
    "mysql2": ">=3.0.0",
    "pg": ">=8.0.0"
  },
  "peerDependenciesMeta": {
    "mongodb": {
      "optional": true
    },
    "pg": {
      "optional": true
    },
    "mysql2": {
      "optional": true
    },
    "better-sqlite3": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.8",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "better-sqlite3": "^12.5.0",
    "eslint": "^8.55.0",
    "mongodb": "^7.0.0",
    "mysql2": "^3.16.0",
    "pg": "^8.16.3",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}
